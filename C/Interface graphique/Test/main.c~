#include <stdio.h>
#include <stdlib.h>
#include <gtk/gtk.h>
#include "mainWindowControler.h"
#include "mainMenuControler.h"
#include "settingsMenuControler.h"
#include "runMenuControler.h"

int main (int argc, char** argv)
{
	gtk_init(&argc, &argv);
	initializeStructGlobal();
	
	mainWindow();
	mainMenu();
	settingsMenu();
	runMenu();
	
	return 0;
}
